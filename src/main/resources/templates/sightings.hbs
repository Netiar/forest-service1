{{#partial "styles"}}
    <link rel="stylesheet" href="/css/index.css">
{{/partial}}

{{#partial "content"}}
    <h2 class="text-center">Animals</h2>
    <table class="table table-striped table-hover">
        <thead class="table-dark">
        <tr>
            <th scope="col">Animal Name</th>
            <th scope="col">Animal Type</th>
            <th scope="col">Age</th>
            <th scope="col">Health>

        </thead>
        <tbody>
        {{#each sightings}}
            <tr>
                <td>{{id}}</td>
                <td>{{EndangeredAnimal}}</td>
                <td>{{location}}</td>
                <td>{{rangerName}}</td>



            </tr>
        {{/each}}
        </tbody>
    </table>
{{/partial}}
{{>layout.hbs}}
